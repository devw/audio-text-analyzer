{
    "editor.formatOnSave": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "files.associations": {
        "**/out**.js": "plaintext"
    },
    "workbench.colorCustomizations": {
        "tab.activeBorderTop": "#000",
        "activityBar.background": "#4da6ff",
        "titleBar.activeBackground": "#4C9B55",
        "titleBar.activeForeground": "#e7e7e7",
        "tab.activeBackground": "#5d20d6",
        "activityBar.activeBackground": "#4da6ff",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#ffdfef",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#4da6ff",
        "statusBar.background": "#4C9B55",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#4C9B55",
        "statusBarItem.remoteBackground": "#4C9B55",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#4C9B5599",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.enable": true,
    "eslint.validate": ["javascript", "javascriptreact", "typescript"],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "peacock.color": "#4C9B55"
}
